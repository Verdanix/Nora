<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd" >

<suite name="NoraCoreTestSuite" verbose="1">
    <test name="logger" preserve-order="true" thread-count="4" parallel="methods">
        <groups>
            <run>
                <include name="info"/>
                <include name="warn"/>
                <include name="error"/>
                <include name="debug"/>
                <include name="trace"/>
            </run>
        </groups>
        <classes>
            <class name="dev.masonroot.nora.NoraLoggerTest"/>
        </classes>
    </test>
</suite>
